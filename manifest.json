{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts":[
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-2.2.4.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["jquery-2.2.4.min.js", "myscript.js"]
    },
  "permissions": [
   "<all_urls>","webRequest","webRequestBlocking"
  ]
}